#include <iostream>;

using namespace std;

void delete_duplicates (int n, int a[n])
{
    for (int i=0; i<n; i++)
    {
        for (int j=i+1; j<n; j++)
        {
            if (a[j]==a[i])
            {
                a[j]='delete';
            }
        }
        if (a[i]!='delete') 
        {
            cout<<a[i];
        }
    }
}
    
    
int main ()
{
    int n;
    cin>>n;
    int* a=new int[n];
    for (int i=0; i<n; i++)
    {
        cin>>a[i];
    }
    delete_duplicates;
    delete[] a;
    return 0;
}
